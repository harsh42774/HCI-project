	    			.success(function(response) 
	    				{
	    					$scope.news=response.News;
	    				});
			}
			else if($routeParams.month){
				$http.get("/api/news/newsArchieveYearMonth?year="+$routeParams.year+"&month="+$routeParams.month)
	    			.success(function(response) 
	    				{
	    					$scope.news=response.News;
	    				});
			}
			else if($routeParams.year){
				$http.get("/api/news/newsArchieveYear?year="+$routeParams.year)
	    			.success(function(response) 
	    				{
	    					$scope.news=response.News;
	    				});
			}
			else{
				$http.get("/api/news/newsArchieve")
	    			.success(function(response) 
	    				{
	    					$scope.news=response.News;
	    				});
